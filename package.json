{
  "name": "devcamper-api",
  "version": "0.0.1",
  "description": "An API for a bootcamp directory",
  "main": "server/app.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/server",
    "dev": "NODE_ENV=development nodemon server/app.js",
    "docker:install": "docker-compose run --rm --no-deps server npm i",
    "docker:start": "docker-compose up",
    "docker:seed:data": "docker-compose run --rm --no-deps server node seeder -i",
    "docker:delete:data": "docker-compose run --rm --no-deps server node seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajmasia/devcamper-api.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Antonio José Masiá",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajmasia/devcamper-api/issues"
  },
  "homepage": "https://github.com/ajmasia/devcamper-api#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "mongoose": "^5.11.9",
    "morgan": "^1.10.0",
    "node-geocoder": "^3.27.0",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
